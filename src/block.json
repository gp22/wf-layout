{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wazframe/wf-layout",
	"version": "0.1.0",
	"title": "Layout",
	"category": "design",
	"icon": "smiley",
	"description": "Every Layout components.",
	"supports": {
		"html": false
	},
	"textdomain": "wf-layout",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"layout": {
			"type": "string",
			"default": "stack"
		},
		"size": {
			"type": "string",
			"default": "default"
		},
		"split": {
			"type": "number",
			"default": 0
		}
	}
}
